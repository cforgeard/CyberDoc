traefik.enable=true
traefik.http.routers.onlyofficerouter.rule=Host(`onlyoffice.fulgen.fr`)
traefik.http.routers.onlyofficerouter.tls=true
traefik.http.routers.onlyofficerouter.tls.certresolver=myresolver
traefik.http.routers.onlyofficerouter.middlewares=onlyoffice-sslheaders,onlyoffice-cors
traefik.http.services.onlyofficeservice.loadbalancer.server.port=80
# https if on secure state - replace http if insecure state
traefik.http.middlewares.onlyoffice-sslheaders.headers.customrequestheaders.X-Forwarded-Proto=https
traefik.http.middlewares.onlyoffice-cors.headers.accessControlAllowOrigin=*
traefik.http.middlewares.onlyoffice-cors.headers.accessControlAllowHeaders=*

