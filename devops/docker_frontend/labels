traefik.enable=true
traefik.http.routers.frontendrouter.rule=Host(`cyberdoc.fulgen.fr`)
traefik.http.routers.frontendrouter.tls=true
traefik.http.routers.frontendrouter.tls.certresolver=myresolver
traefik.http.services.frontendservice.loadbalancer.server.port=80

# maximum rate by second
traefik.http.middlewares.frontendratelimit.ratelimit.average=50
# maximum request allowed to go through in the same arbitrarily small period of time
traefik.http.middlewares.frontendratelimit.ratelimit.burst=50
traefik.http.routers.frontendrouter.middlewares=frontendratelimit@docker"