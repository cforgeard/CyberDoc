traefik.enable=true
traefik.http.routers.redocrouter.rule=Host(`redoc.fulgen.fr`)
traefik.http.routers.redocrouter.tls=true
traefik.http.routers.redocrouter.tls.certresolver=myresolver
traefik.http.services.redocservice.loadbalancer.server.port=80
traefik.http.middlewares.redocratelimit.ratelimit.average=30
traefik.http.middlewares.redocratelimit.ratelimit.burst=15
traefik.http.middlewares.redocratelimit.ratelimit.period=1m
traefik.http.middlewares.redocratelimit.ratelimit.sourcecriterion.requesthost=true
traefik.http.routers.redocrouter.middlewares=redocratelimit@docker
