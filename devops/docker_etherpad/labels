traefik.enable=true
traefik.http.routers.etherpadrouter.rule=Host(`etherpad.cyberdoc.fulgen.fr`)
traefik.http.routers.etherpadrouter.tls=true
traefik.http.routers.etherpadrouter.tls.certresolver=myresolver
traefik.http.services.etherpadservice.loadbalancer.server.port=9001
traefik.http.middlewares.etherpadratelimit.ratelimit.average=100
traefik.http.middlewares.etherpadratelimit.ratelimit.burst=30
traefik.http.middlewares.etherpadratelimit.ratelimit.period=1m
traefik.http.middlewares.etherpadratelimit.ratelimit.sourcecriterion.requesthost=true
traefik.http.routers.etherpadrouter.middlewares=etherpadratelimit@docker
